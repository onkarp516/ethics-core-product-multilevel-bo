{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "browser",
  "documentRoot": "/neucore-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "extensions.*",
    "window.*",
    "storage.*",
    "events.*"
  ],
  "modes": {
    "window": {
      "exitProcessOnClose": false,
      "enableInspector": true,
      "title": "neucore",
      "minWidth": 300,
      "borderless": false,
      "maximize": true,
      "minHeight": 500,
      "fullScreen": true,
      "icon": "/neucore-react/public/logo192.png",
      "nativeAllowList": [
        "app.*",
        "os.*",
        "filesystem.*",
        "extensions.*",
        "window.*",
        "storage.*",
        "events.*"
      ]
    },
    "chrome": {
      "exitProcessOnClose": false,
      "enableInspector": true,
      "title": "Ethics 2.0",
      "fullScreen": true,
      "borderless": true,
      "maximize": true,
      "icon": "/neucore-react/public/logo192.png",
      "nativeAllowList": [
        "app.*",
        "os.*",
        "filesystem.*",
        "extensions.*",
        "window.*",
        "storage.*",
        "events.*"
      ]
    }
  },
  "cli": {
    "binaryName": "neucore",
    "resourcesPath": "/neucore-react/build/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.2.0",
    "clientVersion": "3.1.0",
    "frontendLibrary": {
      "patchFile": "/neucore-react/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
